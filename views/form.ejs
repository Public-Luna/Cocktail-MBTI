<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>example</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
    <ariticle id="start">
        <header class="container text-center" style="height: 70px; line-height: 70px; letter-spacing: 2.5px;">
            <span style="color: white;">M</span><span style="color: #ce85e4;">C</span><span style="color:white;">TI</span>
        </header>
    </ariticle>
    <article class="question container text-center">
        <div class="prog mt-5">
            <div class="progs" style="width: calc(100/12*1%);"></div>
        </div>
        <% question_map.forEach(function(e, i) { %>
            <div class="container text-center q<%=i%>" style="margin-top: 8em; display: none;">
                <h2 class="text-center mt-5 mb-2" style="color: white; font-family: GyeonggiBatang; font-size: 35px;"><%= e.title %></h2>
                <p class="text-center mt-3 mb-5" style="color: white; font-family: GyeonggiBatang; font-size: 23px;"><%- e.content %></p>
                <button onclick="next(0)" class="ansbutton btn-primary mt-5" style="font-size: 13px;"><%- e.A %></button>
                <button onclick="next(1)" class="ansbutton btn-primary mt-2" style="font-size: 13px;"><%- e.B %></button>
            </div>
        <% }) %>
    </article>
    <script src="/javascripts/form.js"></script>
</body>
</html>
